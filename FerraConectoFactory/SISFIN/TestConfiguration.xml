<?xml version="1.0" encoding="UTF-8"?>
<pm-test xmlns="http://sailpoint.com/idm/xms/xbean/test">
    <ms-profiles>
        <ms-profile>
            <name>TesteConnection</name>
            <admin-name>CTSAD</admin-name>
            <def-admin-name>CTSAD</def-admin-name>
            <mscsParm>
                <ms-parameter>
                    <name>url</name>
                    <send>true</send>
                    <value>sdges1.desenvolvimento.extracaixa</value>
                </ms-parameter>
            </mscsParm>
        </ms-profile>
    </ms-profiles>
    <test-profiles>
        <test-profile>
            <name>getUsuario</name>
            <description/>
            <entity>ACCOUNT</entity>
            <action>GET</action>
            <scope>
                <scope-type>account</scope-type>
                <list-values>
                    <value>C002409</value>
                </list-values>
            </scope>
            <run-keywords>
                <keyword>
                    <name>NOME</name>
                </keyword>
                <keyword>
                    <name>UNIDADE</name>
                </keyword>
                <keyword>
                    <name>UNIDADE_ADM</name>
                </keyword>
                <keyword>
                    <name>FUNC_EFET</name>
                </keyword>
                <keyword>
                    <name>FUNC_EVENT</name>
                </keyword>
                <keyword>
                    <name>FUNC_EXERC</name>
                </keyword>
            </run-keywords>
        </test-profile>
        <test-profile>
            <name>getGroups</name>
            <description/>
            <entity>GROUP</entity>
            <action>GET</action>
            <scope>
                <scope-type>all</scope-type>
            </scope>
            <run-keywords>
                <keyword>
                    <name>SISTEMA</name>
                </keyword>
                <keyword>
                    <name>PERFIL</name>
                </keyword>
            </run-keywords>
        </test-profile>
        <test-profile>
            <name>TrocaSenha</name>
            <description/>
            <entity>ACCOUNT</entity>
            <action>UPDATE</action>
            <run-keywords>
                <keyword>
                    <name>XSA_ACCOUNT_NAME</name>
                    <interactive>true</interactive>
                </keyword>
                <keyword>
                    <name>XSA_PASSWORD</name>
                    <interactive>true</interactive>
                </keyword>
            </run-keywords>
        </test-profile>
    </test-profiles>
</pm-test>