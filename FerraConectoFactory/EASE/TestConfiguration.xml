<?xml version="1.0" encoding="UTF-8"?>
<pm-test xmlns="http://sailpoint.com/idm/xms/xbean/test">
    <ms-profiles>
        <ms-profile>
            <name>testConnection</name>
            <admin-name>gal ease</admin-name>
            <def-admin-name>gal ease</def-admin-name>
            <mscsParm>
                <ms-parameter>
                    <name>wsdl</name>
                    <send>true</send>
                    <value>http://10.192.228.181:22080/axis2/services/SAT?wsdl</value>
                </ms-parameter>
            </mscsParm>
        </ms-profile>
    </ms-profiles>
    <test-profiles>
        <test-profile>
            <name>GetAccount</name>
            <description/>
            <entity>ACCOUNT</entity>
            <action>GET</action>
            <scope>
                <scope-type>account</scope-type>
                <list-values>
                    <value>C000004</value>
                </list-values>
            </scope>
            <run-keywords>
                <keyword>
                    <name>NO_USUARIO</name>
                </keyword>
                <keyword>
                    <name>PASSWORD</name>
                </keyword>
                <keyword>
                    <name>CO_PERFIL</name>
                </keyword>
                <keyword>
                    <name>NO_PERFIL</name>
                </keyword>
                <keyword>
                    <name>NO_LOTACAO</name>
                </keyword>
            </run-keywords>
        </test-profile>
        <test-profile>
            <name>connectionRemove</name>
            <description/>
            <entity>CONNECTION</entity>
            <action>DELETE</action>
            <run-keywords>
                <keyword>
                    <name>XSA_ACCOUNT_NAME</name>
                    <value>C000128</value>
                    <interactive>true</interactive>
                </keyword>
                <keyword>
                    <name>XSA_GROUP_NAME</name>
                    <value>xxxx</value>
                    <interactive>true</interactive>
                </keyword>
            </run-keywords>
        </test-profile>
        <test-profile>
            <name>ADD</name>
            <description/>
            <entity>CONNECTION</entity>
            <action>ADD</action>
            <run-keywords>
                <keyword>
                    <name>XSA_ACCOUNT_NAME</name>
                    <interactive>true</interactive>
                </keyword>
                <keyword>
                    <name>XSA_GROUP_NAME</name>
                    <interactive>true</interactive>
                </keyword>
            </run-keywords>
        </test-profile>
        <test-profile>
            <name>AddAccount</name>
            <description/>
            <entity>ACCOUNT</entity>
            <action>UPDATE</action>
            <run-keywords>
                <keyword>
                    <name>XSA_ACCOUNT_NAME</name>
                    <interactive>true</interactive>
                </keyword>
                <keyword>
                    <name>CO_PERFIL</name>
                    <interactive>true</interactive>
                </keyword>
            </run-keywords>
        </test-profile>
    </test-profiles>
</pm-test>